{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["airbnb", "react-app", "react-app/jest"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "quotes": ["error", "double"],
        "import/no-named-as-default": "off",
        "no-plusplus": "off",
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
    }
}
